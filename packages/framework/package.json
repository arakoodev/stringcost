{
  "name": "@stringcost/framework",
  "version": "0.1.0",
  "description": "Core primitives for building metered stringcost agents.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./mock": {
      "import": "./dist/mock/index.mjs",
      "require": "./dist/mock/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/mock/index.ts --dts --format esm,cjs",
    "test": "npm run build && node --test tests/billing.test.js",
    "lint": "tsc --noEmit"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
